{
  "name": "@jimmy.codes/eslint-config",
  "version": "6.14.3",
  "description": "A simple, modern ESLint config that covers most use cases.",
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript",
    "react",
    "nextjs",
    "astro",
    "vitest",
    "tanstack-query",
    "react-compiler",
    "modern",
    "opinionated",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmy-guzman/eslint-config.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./globs": {
      "types": "./dist/globs.d.ts",
      "default": "./dist/globs.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm typegen && tsdown",
    "coverage": "vitest run --coverage",
    "dev": "eslint-config-inspector --open=false",
    "docs:build": "eslint-config-inspector build",
    "format": "prettier --check --cache .",
    "format:fix": "prettier --write --cache .",
    "knip": "knip",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "prepack": "pnpm build && clean-pkg-json",
    "prepare": "is-ci || lefthook install",
    "test": "vitest",
    "typecheck": "tsc",
    "typegen": "tsx scripts/typegen.ts"
  },
  "prettier": "@jimmy.codes/prettier-config",
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint-react/eslint-plugin": "^2.2.4",
    "@eslint/js": "^9.38.0",
    "@next/eslint-plugin-next": "^15.5.6",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@types/eslint": "9.6.1",
    "@typescript-eslint/parser": "^8.46.2",
    "@typescript-eslint/utils": "^8.46.2",
    "@vitest/eslint-plugin": "^1.3.25",
    "astro-eslint-parser": "^1.2.2",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-astro": "^1.4.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsdoc": "^61.1.9",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-dom": "^2.2.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-hooks-extra": "^2.2.4",
    "eslint-plugin-react-naming-convention": "^2.2.4",
    "eslint-plugin-react-refresh": "0.4.24",
    "eslint-plugin-react-web-api": "^2.2.4",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-storybook": "0.12.0",
    "eslint-plugin-testing-library": "^7.13.3",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.4.0",
    "local-pkg": "^1.1.2",
    "typescript-eslint": "^8.46.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "20.0.0",
    "@eslint/config-inspector": "1.3.0",
    "@jimmy.codes/prettier-config": "1.6.0",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/eslint-plugin-jsx-a11y": "6.10.1",
    "@types/node": "22.15.30",
    "@types/react": "19.2.2",
    "@vitest/coverage-v8": "3.2.4",
    "clean-pkg-json": "1.3.0",
    "commitlint": "20.1.0",
    "eslint": "9.38.0",
    "eslint-typegen": "2.3.0",
    "gitzy": "6.3.0",
    "is-ci": "4.1.0",
    "jiti": "2.6.1",
    "knip": "5.66.3",
    "lefthook": "2.0.1",
    "prettier": "3.6.2",
    "publint": "0.3.15",
    "tsdown": "0.15.10",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "eslint": "^9.10.0"
  },
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "node": ">= 20.10.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitzy": {
    "useCommitlintConfig": true
  }
}
