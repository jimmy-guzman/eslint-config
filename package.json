{
  "name": "@jimmy.codes/eslint-config",
  "version": "6.27.0",
  "description": "A simple, modern ESLint config that covers most use cases.",
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript",
    "react",
    "nextjs",
    "astro",
    "vitest",
    "tanstack-query",
    "react-compiler",
    "modern",
    "opinionated",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmy-guzman/eslint-config.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./globs": {
      "types": "./dist/globs.d.mts",
      "default": "./dist/globs.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm typegen && tsdown",
    "coverage": "vitest run --coverage",
    "dev": "eslint-config-inspector --open=false",
    "docs:build": "eslint-config-inspector build",
    "format": "prettier --check --cache .",
    "format:fix": "prettier --write --cache .",
    "knip": "knip",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "prepack": "pnpm build && clean-pkg-json",
    "prepare": "is-ci || lefthook install",
    "test": "vitest",
    "typecheck": "tsc",
    "typegen": "tsx scripts/typegen.ts"
  },
  "prettier": "@jimmy.codes/prettier-config",
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.39.2",
    "@next/eslint-plugin-next": "^16.1.0",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@types/eslint": "9.6.1",
    "@typescript-eslint/parser": "^8.50.0",
    "@typescript-eslint/utils": "^8.50.0",
    "@vitest/eslint-plugin": "^1.5.4",
    "astro-eslint-parser": "^1.2.2",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-arrow-return-style-x": "^1.2.6",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-de-morgan": "^2.0.0",
    "eslint-plugin-erasable-syntax-only": "0.4.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jest": "^29.5.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-perfectionist": "^5.0.0",
    "eslint-plugin-playwright": "^2.4.0",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-dom": "^2.3.13",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-hooks-extra": "^2.3.13",
    "eslint-plugin-react-naming-convention": "^2.3.13",
    "eslint-plugin-react-refresh": "0.4.26",
    "eslint-plugin-react-web-api": "^2.3.13",
    "eslint-plugin-react-x": "^2.3.13",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-storybook": "0.12.0",
    "eslint-plugin-testing-library": "^7.15.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.5.0",
    "local-pkg": "^1.1.2",
    "typescript-eslint": "^8.50.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "20.2.0",
    "@eslint/config-inspector": "1.4.2",
    "@jimmy.codes/prettier-config": "2.1.2",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/eslint-plugin-jsx-a11y": "6.10.1",
    "@types/node": "24.10.3",
    "@types/react": "19.2.7",
    "@vitest/coverage-v8": "4.0.16",
    "clean-pkg-json": "1.3.0",
    "commitlint": "20.2.0",
    "eslint": "9.39.2",
    "eslint-typegen": "2.3.0",
    "gitzy": "6.5.7",
    "is-ci": "4.1.0",
    "jiti": "2.6.1",
    "knip": "5.76.1",
    "lefthook": "2.0.12",
    "prettier": "3.7.4",
    "publint": "0.3.16",
    "tsdown": "0.18.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  },
  "peerDependencies": {
    "eslint": "^9.10.0"
  },
  "packageManager": "pnpm@10.26.1",
  "engines": {
    "node": ">= 20.10.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitzy": {
    "useCommitlintConfig": true
  }
}
