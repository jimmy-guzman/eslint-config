{
  "name": "@jimmy.codes/eslint-config",
  "version": "1.11.0",
  "description": "another opinionated eslint config",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslintconfig"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jimmy-guzman/eslint-config.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "typegen": "tsx scripts/typegen.ts",
    "build": "pnpm typegen && pkgroll --minify",
    "coverage": "vitest run --coverage",
    "dev": "pkgroll --watch",
    "docs:build": "npx @eslint/config-inspector build",
    "docs:view": "npx @eslint/config-inspector --open=false",
    "format": "prettier --check --cache .",
    "format:fix": "prettier --write --cache .",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "prepack": "pnpm build && clean-pkg-json",
    "prepare": "is-ci || lefthook install",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "prettier": "@jimmy.codes/prettier-config",
  "dependencies": {
    "@eslint/js": "^8.57.0",
    "@tanstack/eslint-plugin-query": "^5.28.11",
    "@typescript-eslint/utils": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "astro-eslint-parser": "^1.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-astro": "^1.3.0",
    "eslint-plugin-import-x": "^4.4.0",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node-import": "^1.0.4",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "0.4.14",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "globals": "^15.11.0",
    "local-pkg": "0.5.0",
    "typescript-eslint": "^7.6.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "19.5.0",
    "@eslint/config-inspector": "0.5.6",
    "@jimmy.codes/prettier-config": "1.1.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "14.0.1",
    "@tanstack/react-query": "5.59.16",
    "@testing-library/dom": "10.1.0",
    "@testing-library/react": "16.0.0",
    "@types/eslint": "8.56.10",
    "@types/eslint__js": "8.42.3",
    "@types/node": "22.8.4",
    "@types/react": "18.3.12",
    "@vitest/coverage-v8": "2.1.4",
    "astro": "4.16.7",
    "clean-pkg-json": "1.2.0",
    "commitlint": "19.5.0",
    "eslint": "8.57.0",
    "eslint-typegen": "0.2.4",
    "gitzy": "5.4.0",
    "is-ci": "3.0.1",
    "jiti": "2.3.3",
    "lefthook": "1.8.2",
    "pkgroll": "2.5.1",
    "prettier": "3.3.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "semantic-release": "24.2.0",
    "tsx": "4.19.2",
    "typescript": "5.4.5",
    "vitest": "2.1.4"
  },
  "peerDependencies": {
    "eslint": "^8.57.0"
  },
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">= 20"
  },
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint-plugin-import"
      ]
    }
  },
  "gitzy": {
    "useCommitlintConfig": true
  }
}
